generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File_Storage {
  id Int @id @default(autoincrement())
  filename String
  content Bytes
  author Users @relation(fields: [authorId], references: [id])
  authorId Int
}

model Users {
  id  Int @id @default(autoincrement())
  username String @unique
  password String
  files File_Storage[]
}